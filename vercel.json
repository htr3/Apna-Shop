{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "env": [
    "DATABASE_URL",
    "NODE_ENV",
    "JWT_SECRET",
    "WHATSAPP_API_KEY",
    "VITE_API_URL"
  ],
  "functions": {
    "server/index.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}

